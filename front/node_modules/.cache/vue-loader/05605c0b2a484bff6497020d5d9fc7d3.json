ialog',this.messageDialog);\r\n            },\r\n            dialog:function(){\r\n                this.messageDialog=this.dialog;\r\n            }\r\n        },\r\n        methods:{\r\n            formatJson(message){\r\n                if(message.length!=0)\r\n                {\r\n                    if(message[0]=='{')\r\n                    {\r\n                        return JSON.stringify(JSON.parse(message),null,2);\r\n\r\n                    }\r\n                    else\r\n                    {\r\n                        return message;\r\n                    }\r\n                }\r\n            }\r\n        }\r\n    }\r\n</script>\r\n"]}]}